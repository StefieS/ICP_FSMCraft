{
  "name": "TOF5s",
  "inputs": [
    "in"
  ],
  "outputs": [
    "out"
  ],
  "internal": [
    {
      "name": "timeout",
      "type": "int",
      "initialValue": "5000"
    },
    {
      "name": "other",
      "type": "str",
      "initialValue": "hello"
    }
  ],
  "states": [
    {
      "name": "IDLE",
      "action": "fsm.output(\"out\", 0)",
      "isInitial": true,
      "isFinal": false
    },
    {
      "name": "ACTIVE",
      "action": "fsm.output(\"out\", 1)",
      "isInitial": false,
      "isFinal": false
    },
    {
      "name": "TIMING",
      "action": "",
      "isInitial": false,
      "isFinal": false
    }
  ],
  "transitions": [
    {
      "src": "IDLE",
      "dst": "ACTIVE",
      "input": "in",
      "cond": "in == 1"
    },
    {
      "src": "ACTIVE",
      "dst": "TIMING",
      "input": "in",
      "cond": "in == 0"
    },
    {
      "src": "TIMING",
      "dst": "ACTIVE",
      "input": "in",
      "cond": "in == 1"
    },
    {
      "src": "TIMING",
      "dst": "IDLE",
      "input": "",
      "cond": "timeout"
    }
  ]
}