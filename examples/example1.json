{
  "name": "TOF5s",
  "inputs": [
    "in"
  ],
  "outputs": [
    "out"
  ],
  "internal": [
    {
      "name": "timeout",
      "type": "int",
      "initialValue": "5000"
    },
    {
      "name": "other",
      "type": "str",
      "initialValue": "hello"
    }
  ],
  "states": [
    {
      "name": "IDLE",
      "action": "output(\"out\", 0)",
      "initial": "1",
      "final": "0"
    },
    {
      "name": "ACTIVE",
      "action": "output(\"out\", 1)",
      "initial": "0",
      "final": "0"
    },
    {
      "name": "TIMING",
      "action": "",
      "initial": "0",
      "final": "0"
    }
  ],
  "transitions": [
    {
      "src": "IDLE",
      "dst": "ACTIVE",
      "input": "in",
      "cond": "atoi(valueof(\"in\")) == 1"
    },
    {
      "src": "ACTIVE",
      "dst": "TIMING",
      "input": "in",
      "cond": "atoi(valueof(\"in\")) == 0"
    },
    {
      "src": "TIMING",
      "dst": "ACTIVE",
      "input": "in",
      "cond": "atoi(valueof(\"in\")) == 1"
    },
    {
      "src": "TIMING",
      "dst": "IDLE",
      "input": "@",
      "cond": "timeout"
    }
  ]
}